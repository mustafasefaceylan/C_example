#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int power(int a, int b)
{
	int result = 1;

	while (b--)
		result *= a;

	return result;
}



int main()
{
	int base, exp;

	printf("taban ve us degerini giriniz: ");
	scanf("%d%d", &base, &exp);

	printf("%d ---> %d = %d\n", base, exp, power(base, exp));

}
